<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="FeatureAnalyzerPlugin.commands.category"
            name="%category.name">
      </category>
      <command
            categoryId="FeatureAnalyzerPlugin.commands.category"
            id="br.ufal.ic.featureanalyzerplugin.commands.FeatureAnalyzerCommand"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="br.ufal.ic.featureanalyzer.handler.FeatureAnalyzerPluginHandler"
            commandId="br.ufal.ic.featureanalyzerplugin.commands.FeatureAnalyzerCommand">
      </handler>
   </extension>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="%category.name.0"
            id="FeatureAnalyzerPlugin">
      </category>
      <view
            name="%view.name"
            icon="icons/featured.png"
            category="FeatureAnalyzerPlugin"
            class="br.ufal.ic.featureanalyzer.views.AnalyzerView"
            id="br.ufal.ic.featureanalyzer.views.AnalyzerView">
      </view>
      <view
            category="FeatureAnalyzerPlugin"
            class="br.ufal.ic.featureanalyzer.views.InvalidProductView"
            icon="icons/invalidproduct.png"
            id="br.ufal.ic.featureanalyzer.views.invalideproductview"
            name="%invalidproducview.name">
      </view>
   </extension>
 
    <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="br.ufal.ic.featureanalyzer.preferences.PreferencePage"
            id="br.ufal.ic.featureanalyzer.preferences.PreferencePage"
            name="%page.name">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="br.ufal.ic.featureanalyzer.preferences.Initializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="br.ufal.ic.typechefplugin.toolbars.sampleToolbar">
            <command
                  commandId="br.ufal.ic.featureanalyzerplugin.commands.FeatureAnalyzerCommand"
                  icon="icons/featured.png"
                  tooltip="%command.tooltip"
                  id="br.ufal.ic.featureanalyzerplugin.toolbars.FeatureAnalyzerCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   	<extension
id="br.ufal.ic.featureanalyzer.problem"
name="%view.name"
point="org.eclipse.core.resources.markers">
<super type="org.eclipse.core.resources.problemmarker" />
<persistent value="true" />
</extension>
   <extension
         point="de.ovgu.featureide.core.composers">
      <composer
            class="br.ufal.ic.featureanalyzer.activator.CPPComposer"
            description="C composer to FeatureIDE"
            id="br.ufal.ic.featureanalyzer.cppcomposer"
            name="C PreProcessors "
            nature="FeatureIDE_Cnature">
      </composer>
   </extension>
   <extension
         point="de.ovgu.featureide.fm.core.FMComposer">
      <fmComposer
            class="br.ufal.ic.featureanalyzer.activator.CPPModelBuilder"
            id="br.ufal.ic.featureanalyzer.fmcppcomposer"
            name="C PreProcessor Feature Model">
      </fmComposer>
   </extension>
   <extension
         id="br.ufal.ic.featureanalyzer.invalidproduct"
         name="%invalidproducview.name"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
</plugin>
