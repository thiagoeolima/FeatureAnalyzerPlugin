<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="br.ufal.ic.featureanalyzer.commands.category"
            name="Feature Analyzer">
      </category>
      <command
            categoryId="br.ufal.ic.featureanalyzer.commands.category"
            id="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerCommand"
            name="Analyzer">
      </command>
      <command
            id="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerViewClear"
            name="Clear View">
      </command>
      <command
            id="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerStatistics"
            name="Statistics">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="br.ufal.ic.featureanalyzer.handler.FeatureAnalyzerPluginHandler"
            commandId="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerCommand">
      </handler>
      <handler
            class="br.ufal.ic.featureanalyzer.handler.ClearAnalyzerViewHandler"
            commandId="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerViewClear">
      </handler>
      <handler
            class="br.ufal.ic.featureanalyzer.handler.StatisticsHandler"
            commandId="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerStatistics">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="br.ufal.ic.typechefplugin.toolbars.sampleToolbar">
            <command
                  commandId="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerCommand"
                  icon="icons/featured.png"
                  tooltip="Feature Analyzer"
                  id="br.ufal.ic.featureanalyzer.toolbars.FeatureAnalyzerCommand">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:br.ufal.ic.featureanalyzer.views.AnalyzerView">
         <command
               commandId="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerCommand"
               icon="icons/featured_run.png"
               label="Analyse"
               style="push">
         </command>
         <separator
               name="br.ufal.ic.featureanalyzer.separator1"
               visible="true">
         </separator>
         <command
               commandId="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerViewClear"
               icon="icons/featured_clear.png"
               style="push">
         </command>
         <separator
               name="br.ufal.ic.featureanalyzer.separator1"
               visible="true">
         </separator>
         <command
               commandId="br.ufal.ic.featureanalyzer.commands.FeatureAnalyzerStatistics"
               icon="icons/featured_chart.png"
               style="push">
         </command>
      </menuContribution>
   </extension>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="FeatureAnalyzer"
            id="br.ufal.ic.featureanalyzer">
      </category>
      <view
            allowMultiple="false"
            category="br.ufal.ic.featureanalyzer"
            class="br.ufal.ic.featureanalyzer.views.AnalyzerView"
            icon="icons/featured.png"
            id="br.ufal.ic.featureanalyzer.views.AnalyzerView"
            name="Feature Analyzer">
      </view>
      <view
            allowMultiple="false"
            category="br.ufal.ic.featureanalyzer"
            class="br.ufal.ic.featureanalyzer.views.InvalidProductView"
            icon="icons/invalidproduct.png"
            id="br.ufal.ic.featureanalyzer.views.invalideproductview"
            name="Invalid Products">
      </view>
      <view
            allowMultiple="false"
            category="br.ufal.ic.featureanalyzer"
            class="br.ufal.ic.featureanalyzer.views.StatisticsView"
            icon="icons/featured_chart.png"
            id="br.ufal.ic.featureanalyzer.views.StatisticsView"
            name="Statistics">
      </view>
   </extension>
 
    <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="br.ufal.ic.featureanalyzer.preferences.PreferencePage"
            id="br.ufal.ic.featureanalyzer.preferences.PreferencePage"
            name="Feature Analyzer">
      </page>
      <page
            category="br.ufal.ic.featureanalyzer.preferences.PreferencePage"
            class="br.ufal.ic.featureanalyzer.preferences.PreferenceGcc"
            id="br.ufal.ic.featureanalyzer.preferences.PreferenceGcc"
            name="GCC C Compiler Settings">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="br.ufal.ic.featureanalyzer.preferences.Initializer">
      </initializer>
   </extension>
   	<extension
id="br.ufal.ic.featureanalyzer.problem"
name="Feature Analyzer"
point="org.eclipse.core.resources.markers">
<super type="org.eclipse.core.resources.problemmarker" />
<persistent value="true" />
</extension>
   <extension
         point="de.ovgu.featureide.core.composers">
      <composer
            class="br.ufal.ic.featureanalyzer.core.CPPComposer"
            description="C composer to FeatureIDE"
            id="br.ufal.ic.featureanalyzer.cppcomposer"
            name="C PreProcessors "
            nature="FeatureIDE_Cnature">
      </composer>
   </extension>
   <extension
         point="de.ovgu.featureide.fm.core.FMComposer">
      <fmComposer
            class="br.ufal.ic.featureanalyzer.core.CppFMComposerExtension"
            composer="br.ufal.ic.featureanalyzer.cppcomposer"
            id="br.ufal.ic.featureanalyzer.fmcppcomposer"
            name="C PreProcessor Feature Model">
      </fmComposer>
   </extension>
   <extension
         id="br.ufal.ic.featureanalyzer.invalidproduct"
         name="Invalid Products"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
</plugin>
